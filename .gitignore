# Root-level .gitignore for ccelectrical-application monorepo

# Frontend specific (React)
frontend/node_modules/
frontend/build/
frontend/coverage/
frontend/.pnp
frontend/.pnp.js
frontend/.env.local
frontend/.env.development.local
frontend/.env.test.local
frontend/.env.production.local
frontend/npm-debug.log*
frontend/yarn-debug.log*
frontend/yarn-error.log*

# Backend specific (Spring Boot/Maven)
backend/target/
backend/HELP.md
backend/.apt_generated
backend/.classpath
backend/.factorypath
backend/.project
backend/.settings
backend/.springBeans
backend/.sts4-cache
backend/build/

# IDE - IntelliJ IDEA
.idea/
*.iws
*.iml
*.ipr

# IDE - VS Code
.vscode/
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json

# IDE - Eclipse
.project
.classpath
.settings/
.metadata/

# IDE - NetBeans
/nbproject/private/
/nbbuild/
/dist/
/nbdist/
/.nb-gradle/

# OS specific
.DS_Store
Thumbs.db
Desktop.ini

# Logs
*.log
logs/

# Environment variables
.env
.env.local
.env.*.local

# Temporary files
*.tmp
*.temp
*.swp
*.swo
*~

# Package files
*.jar
*.war
*.nar
*.ear
*.zip
*.tar.gz
*.rar

# Exclude build artifacts but keep Maven wrapper
!backend/.mvn/wrapper/maven-wrapper.jar

# Scanner/SonarQube
.scannerwork/

# Keep important documentation
!README.md
!frontend/README.md
!backend/README.md

